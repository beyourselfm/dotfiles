# focus window
ctrl + cmd - h : yabai -m window --focus west
ctrl + cmd - j : yabai -m window --focus south
ctrl + cmd - k : yabai -m window --focus north
ctrl + cmd - l : yabai -m window --focus east

# move by direction
alt + ctrl - h : yabai -m window --swap west
alt + ctrl - j : yabai -m window --swap south
alt + ctrl - k : yabai -m window --swap north
alt + ctrl - l : yabai -m window --swap east
